{% extends 'base.html.twig' %}

{% block title %}Certification{% endblock %}

{% block body %}
    <h1>Certification</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ certification.id }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ certification.type }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ certification.code }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ certification.name }}</td>
            </tr>
            <tr>
                <th>StartDate</th>
                <td>{{ certification.startDate ? certification.startDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>EndDate</th>
                <td>{{ certification.endDate ? certification.endDate|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_certification_index') }}">back to list</a>

    <a href="{{ path('app_certification_edit', {'id': certification.id}) }}">edit</a>

    {{ include('certification/_delete_form.html.twig') }}
{% endblock %}
